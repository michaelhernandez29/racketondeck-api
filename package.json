{
  "name": "racketondeck-api",
  "version": "1.0.0",
  "description": "Backend repository for RacketOnDeck, a platform for managing racket sports academies. Built with Express, PostgreSQL, and MongoDB, it handles user authentication, court bookings, notifications, and other essential APIs. Includes WebSocket support for real-time updates and integration with Google Cloud and Kubernetes for scalability.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "check": "prettier --check \"**/*.{js,json,md}\"",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "lint": "eslint \"**/*.{js,json}\"",
    "lint:fix": "eslint \"**/*.{js,json}\" --fix",
    "start": "node --watch-path=src --inspect=9230 --env-file=.env src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/michaelhernandez29/racketondeck-api.git"
  },
  "author": "Michael Hernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelhernandez29/racketondeck-api/issues"
  },
  "homepage": "https://github.com/michaelhernandez29/racketondeck-api#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "eslint-plugin-jsdoc": "^50.3.1",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  }
}
